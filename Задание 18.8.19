number = int(input("Сколько билетов желаете приобраетси?: ")) #вводим количество билетов
price = (0, 990, 1390) #кортеж с ценами билетов в зависимости от возраста
sum_price = 0 #счётчик суммы стоимости всех билетов
for a in range(number):
    age = int(input("Введите возраст посетителя: ")) #вводим возраст посетителя
    if age < 18: #если возраст меньше 18
        sum_price += price[0] #то билет бесплатно (и т.д. по логике)
    elif 18 <= age <= 25:
        sum_price += price[1]
    elif age > 25:
        sum_price += price[2]
if number > 3: #если покупается больше 3 билетов, то даётся скидка 10%
    total = 0.9 * sum_price
else:
    total = sum_price
print("Общая стоимость билетов составляет:", total)
